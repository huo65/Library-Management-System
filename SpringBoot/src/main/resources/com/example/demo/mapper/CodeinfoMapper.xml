<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CodeinfoMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.entity.Codeinfo">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="isbn" column="isbn" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="author" column="author" jdbcType="VARCHAR"/>
            <result property="publisher" column="publisher" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="DATE"/>
            <result property="status" column="status" jdbcType="VARCHAR"/>
    </resultMap>

</mapper>
