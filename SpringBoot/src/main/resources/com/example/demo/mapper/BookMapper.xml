<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.BookMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.entity.Book">
            <id property="isbn" column="isbn" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="author" column="author" jdbcType="VARCHAR"/>
            <result property="publisher" column="publisher" jdbcType="VARCHAR"/>
            <result property="leftNumber" column="left_number" jdbcType="BIGINT"/>
            <result property="totalNumber" column="total_number" jdbcType="BIGINT"/>
            <result property="borrownum" column="borrownum" jdbcType="INTEGER"/>
    </resultMap>


</mapper>
